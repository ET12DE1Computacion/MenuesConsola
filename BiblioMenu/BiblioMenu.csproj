<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Version>1.1</Version>
    <PackageProjectUrl>https://github.com/ET12DE1Computacion/MenuesConsola</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ET12DE1Computacion/MenuesConsola</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Authors>Luis Duran</Authors>
    <Description>Proyecto para facilitar la creaci√≥n de menues de consola</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageIcon>Logo.png</PackageIcon>
    <PackageIconUrl />
    <PackageTags>et12 san martin tecnica menu menues consola et12.edu.ar</PackageTags>
    <Company>et12.edu.ar</Company>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>bin\BiblioMenu.xml</DocumentationFile>
    <OutputPath>bin\</OutputPath>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>C:\Proyectos\MenuesConsola\BiblioMenu\BiblioMenu.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\doc\Logo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
